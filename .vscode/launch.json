{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Listen for XDebug",
			"type": "php",
			"request": "launch",
			"port": 9000,
			"hostname": "0.0.0.0",
			"pathMappings": {
				"/var/www/html": "${workspaceFolder}/www"
			},
			"xdebugSettings": {
				"max_data": 1024,
				"show_hidden": 1,
				"max_children": 128,
				"max_depth": 2
			}
		}
	]
}